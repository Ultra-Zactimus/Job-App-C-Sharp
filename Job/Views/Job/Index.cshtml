<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Webus Jobis Maximus/JOBS</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
  </head>
  <body>
    <h1>NAVIGATION-ETH! JOBIS LISTUS! ... MAXIMUS!!!!!</h1>
    @using Job.Models;
    <h3>Your Search Results</h3>
    <ul>
      @foreach(Jobs jobs in Model)
      {
        <li>Title: @jobs.Title</li>
        <li>Description: @jobs.Description</li>
        <li>Contact Info: @jobs.Contact</li>
      } 
    </ul>
    
    <a href="/jobs/create">Would you like to Apply?</a>
  </body>
</html>